   "source": [
    "from clearml import Task\n",
    "\n",
    "# Get all archived tasks\n",
    "archived_tasks = Task.get_tasks(\n",
    "    tags=[\"test\"]\n",
    "    \n",
    ")\n",
    "\n",
    "for task in archived_tasks:\n",
    "    if task.get_archived():\n",
    "        print(\"we are processing task\",task.id)\n",
    "        task.delete()\n",
    "        print(\"we archived the task\",task.id)\n"
   ]
  }
